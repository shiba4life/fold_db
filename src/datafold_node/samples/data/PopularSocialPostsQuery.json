{
  "type": "query",
  "schema": "SocialMediaPost",
  "fields": ["user_id", "content", "likes", "timestamp"],
  "filter": {
    "operator": "and",
    "conditions": [
      { "field": "likes", "operator": "gt", "value": 100 },
      { "field": "timestamp", "operator": "gt", "value": "2023-06-01" }
    ]
  }
}
